@model IEnumerable<makeup1.Models.Photo>

@{
    ViewBag.Title = "Index";
}

<h2>Photos</h2>

<div class="row">
    @foreach (var p in Model)
    {
        <div class="col-md-4">
            <div style="display:block; background: url(@p.photo); background-size: cover; background-repeat no-repeat; background-position 50% 50%; max-width 100%; height 200px;" class="photo-wrapper"></div>
        </div>
    }
</div>
